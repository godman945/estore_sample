<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >


<mapper namespace="com.fet.db.mybatis.mapper.CoMasterMapper">
	
	
	<resultMap id="BaseResultMap" type="com.fet.db.mybatis.entity.CoMasterEntity">
		<id column="CONO" property="cono" jdbcType="VARCHAR" />
		<id column="CO_TYPE" property="coType" jdbcType="VARCHAR" />
	</resultMap>

	<select id="findAllCoMaster" resultMap="BaseResultMap" resultType="com.fet.db.mybatis.entity.CoMasterEntity">
		select CONO,CO_TYPE from co_master where 1=1 and rownum &lt; 10
	</select>
	
	
	
	
	
	
	
</mapper>