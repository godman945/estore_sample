<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >


<mapper namespace="com.fet.db.mybatis.mapper.EstoreTutorialAlexMapper">
	
	
	<resultMap id="BaseResultMap" type="com.fet.db.mybatis.entity.EstoreTutorialAlexEntity">
		<id column="ID" property="id" jdbcType="VARCHAR" />
	</resultMap>

	<select id="getAllEstoreTutorialAlex" resultMap="BaseResultMap" resultType="com.fet.db.mybatis.entity.EstoreTutorialAlexEntity">
		select * from ESTORE_TUTORIAL_ALEX
	</select>
	
	
	<select id="getAllEstoreTutorialAlexByName" resultMap="BaseResultMap" resultType="com.fet.db.mybatis.entity.EstoreTutorialAlexEntity">
