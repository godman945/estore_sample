var ad2udid,expire,ad2trk;function trackAdEvent(e,t,r){var a=_getCookie("ad2udid"),n=_getCookie("ad2trk"),o=_getCookie("ad2s");null==a&&(a="unknown"),null==n&&(n="");var i=encodeURIComponent(window.location.search.substring(1)),d=document.createElement("script");d.type="application/javascript",d.src="https://adlogs.ad2iction.com/tracker/event/?udid="+a+"&en="+e+"&enc="+t+"&desc="+r+"&parameters="+i+"&ad2trk="+n+"&ad2s="+o,document.getElementsByTagName("head")[0].appendChild(d)}function _getURLParameter(e){return decodeURIComponent((new RegExp("[?|&]"+e+"=([^&;]+?)(&|#|;|$)").exec(location.search)||[,""])[1].replace(/\+/g,"%20"))||null}function _getCookie(e){var t,r,a=null,n=e+"=",o=document.cookie,i=o.indexOf(n);return-1!=i&&(t=i+n.length,-1==(r=o.indexOf(";",t))&&(r=o.length),a=decodeURIComponent(o.substring(t,r))),a}function _ad2uuid(){var r=Date.now();return"undefined"!=typeof performance&&"function"==typeof performance.now&&(r+=performance.now()),"xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx".replace(/[xy]/g,function(e){var t=(r+16*Math.random())%16|0;return r=Math.floor(r/16),("x"===e?t:3&t|8).toString(16)})}myvar=_getURLParameter("ad2udid"),trackerSource=_getURLParameter("ad2s"),null!=myvar?((expire=new Date).setTime(expire.getTime()+15552e6),document.cookie="ad2udid="+myvar+"; expires="+expire.toUTCString()+"; path=/"):(ad2udid=_getCookie("ad2udid"),(expire=new Date).setTime(expire.getTime()+15552e6),null==ad2udid||(document.cookie="ad2udid="+ad2udid+"; expires="+expire.toUTCString()+"; path=/")),null!=trackerSource&&((expire=new Date).setTime(expire.getTime()+15552e6),document.cookie="ad2s="+trackerSource+"; expires="+expire.toUTCString()+"; path=/",null==(ad2trk=_getCookie("ad2trk"))?ad2trk=trackerSource+"_"+_ad2uuid():-1==ad2trk.indexOf(trackerSource)&&(ad2trk=trackerSource+"_"+ad2trk),(expire=new Date).setTime(expire.getTime()+86400000),document.cookie="ad2trk="+ad2trk+"; expires="+expire.toUTCString()+"; path=/");